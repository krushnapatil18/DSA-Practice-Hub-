package dsa;

public class Two_Sum {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a[] = {2,6,5,8,11};
		int target = 14;
		int n = a.length;
		
		for(int i=0;i<n;i++) {
			for(int j=0;j<n;j++) {
				if(i==j) continue;
				if(a[i]+a[j]==target) {
					
					System.out.println("Yes! Values Are: "+ a[i]+","+a[j]);
					System.out.println("Indexes are: "+i+","+j);
					return;
				}
			}
		}
		System.out.println("NO Values to get target!");

	}

}
