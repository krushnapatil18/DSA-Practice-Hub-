package dsa;

public class Reverse_Pairing {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int arr[] = {40,25,19,12,9,6,2};
		int n = arr.length;
		int count = 0;
		
		for(int i=0;i<n;i++) {
			for(int j=i+1;j<n;j++) {
				if(arr[i]>(arr[j]*2)) count++;
			}
		}
		System.out.println(count);


	}

}
